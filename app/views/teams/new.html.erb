<div class="container-fluid">
  <div class="row">
    <div class="col-md-4 col-md-offset-4 col-xs-12 new-team-form form">
      <%= form_for @team do |f| %>
        <div class="form-group">
          Branch of service:
          <select class="select-field" name="team[branch]" id="military-branch">
          <option value="Air Force">Air Force</option>
          <option value="Army">Army</option>
          <option value="Navy">Navy</option>
          <option value="Marine Corps">Marine Corps</option>
        </select>
        </div>
        <div class="form-group">
          <input type="text" class="form-control"<%= f.text_field :name, placeholder: "Team Name" %>
        </div>
        <div class="form-group">
          Your home base:
          <select class="select-field" name="team[start_location]" id="start-location-selector">
          <option value="Fort Bragg, NC">Fort Bragg, NC</option>
          <option value="Fort Campbell, KY">Fort Campbell, KY</option>
          <option value="Fort Hood, TX">Fort Hood, TX</option>
          <option value="Joint Base Lewis-McChord, WA">Joint Base Lewis-McChord, WA</option>
          <option value="Fort Benning, GA">Fort Benning, GA</option>
          <option value="Fort Jackson, SC">Fort Jackson, SC</option>
          <option value="Camp Pendleton, CA">Camp Pendleton, CA</option>
          <option value="Camp Lejeune, NC">Camp Lejeune, NC</option>
          <option value="MCB Hawaii, HI">MCB Hawaii, HI</option>
          <option value="MCAGCC 29 Palms, CA">MCAGCC 29 Palms, CA</option>
          <option value="NS Norfolk, VA">NS Norfolk, VA</option>
          <option value="NS Pearl Harbor-Hickam, HI">NS Pearl Harbor-Hickam, HI</option>
          <option value="NAS Jacksonville, FL">NAS Jacksonville, FL</option>
          <option value="NB San Diego, CA">NB San Diego, CA</option>
          <option value="NB Coronado, CA">NB Coronado, CA</option>
          <option value="JB Langley-Eustis, VA">JB Langley-Eustis, VA</option>
          <option value="Hulburt Field, FL">Hulburt Field, FL</option>
          <option value="Barksdale AFB, LA">Barksdale AFB, LA</option>
          <option value="Scott AFB, IL">Scott AFB, IL</option>
        </select>
        </div>
        <div class="form-group">
        Country of deployment:
          <select class="select-field" name="team[end_location]" id="team-end-location">
          <option value="Japan">Japan</option>
          <option value="Afghanistan">Afghanistan</option>
          <option value="Kuwait">Kuwait</option>
          <option value="U.A.E.">U.A.E.</option>
          <option value="Iraq">Iraq</option>
          <option value="Germany">Germany</option>
          <option value="South Korea">South Korea</option>
          <option value="Italy">Italy</option>
          <option value="Guam">Guam</option>
          <option value="Okinawa">Okinawa</option>
          </select>
        </div>
        <div class="form-group">
          <select class="select-field" name="team[months]" id="deployment-length">
            <option value="1">1</option>
            <option value="2">2</option>
            <option value="3">3</option>
            <option value="4">4</option>
            <option value="5">5</option>
            <option value="6">6</option>
            <option value="7">7</option>
            <option value="8">8</option>
            <option value="9">9</option>
            <option value="10">10</option>
            <option value="11">11</option>
            <option value="12">12</option>
            <option value="13">13</option>
            <option value="14">14</option>
            <option value="15">15</option>
            <option value="16">16</option>
          </select>
          months
        </div>
        <div class="form-group">
          <input type="number" class="form-control"<%= f.number_field :goal, placeholder: "Mileage Goal" %>
        </div>
        <%= f.submit :class => "btn", :id => "new-team" %>
      <% end %>
    </div>
  </div>
</div>
<script>

</script>