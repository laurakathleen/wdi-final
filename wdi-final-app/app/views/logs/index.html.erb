<%= render "./users/tabs" %>

<div class="container">
	<div class="row calendar-top">
		<div class="calendar-container calendar-square col-md-12 col-xs-12">
			<%= month_calendar events: @logs do |created_at, logs| %>
  			<%= created_at.strftime("%a %b %d, %Y") %>
  			<% logs.each do |log| %>
    			<div id="calendar-square">
      			<%= link_to log.mileage, @log %> miles
    			</div>
  			<% end %>
			<% end %>
		</div>
	</div>
</div>